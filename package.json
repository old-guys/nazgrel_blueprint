{
  "name": "nazgrel_blueprint",
  "version": "1.0.0",
  "description": "bi doc api",
  "author": "",
  "homepage": "git@gitlab.ishanggang.com:bi/nazgrel_blueprint.git",
  "copyright": "Copyright 2017 creativeLabs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start_channel": "drakov --config channel_config.js",
    "generate_channel": "aglio -i ./source/channel.md -o ./public/channel.html",
    "test_channel": "dredd ./source/api/channel/**/*.md http://127.0.0.1:3006",

    "start_mobile": "drakov --config mobile_config.js",
    "generate_mobile": "aglio -i ./source/mobile.md -o ./public/mobile.html",
    "test_mobile": "dredd ./source/api/mobile/**/*.md http://127.0.0.1:3007",

    "start_web": "drakov --config web_config.js",
    "generate_web": "aglio -i ./source/web.md -o ./public/web.html",
    "test_web": "dredd ./source/api/web/**/*.md http://127.0.0.1:3008",

    "clean": "rimraf ./public/*.html"
  },
  "engines": {
    "node": ">= 5.0.0",
    "npm": ">= 3.0.0"
  },
  "devDependencies": {
    "aglio": "^2.3.0",
    "drakov": "^1.0.4",
    "dredd": "^4.6.2",
    "rimraf": "^2.6.2"
  }
}
